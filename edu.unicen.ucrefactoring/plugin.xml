<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://ucrefactoring/1.0"
            class="edu.unicen.ucrefactoring.model.UCRefactoringPackage"
            genModel="model/ucrefactoring.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser
            type="ucrefactoring"
            class="edu.unicen.ucrefactoring.model.util.UCRefactoringResourceFactoryImpl"/>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="edu.unicen.ucrefactoring.gui.UCPerspective"
            id="edu.unicen.ucrefactoring.perspective1"
            name="UCPerspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="edu.unicen.ucrefactoring.gui.UCView"
            id="edu.unicen.ucrefactoring.UCView"
            name="UCView"
            restorable="true">
      </view>
   </extension>

</plugin>
